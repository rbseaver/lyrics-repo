{
	"configurations": [
		{
			"type": "node",
			"name": "vscode-jest-tests.v2.lyrics-repo",
			"request": "launch",
			"args": [
				"test",
				"--runInBand",
				"--watchAll=false",
				"--testNamePattern",
				"${jest.testNamePattern}",
				"--runTestsByPath",
				"${jest.testFile}"
			],
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"runtimeExecutable": "yarn"
		}
	]
}